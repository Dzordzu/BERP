<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Text>
        <Text strokeType="OUTSIDE" strokeWidth="0.0">
            <HBox.margin>
                <Insets left="40.0" />
            </HBox.margin>
        </Text>
        <Button mnemonicParsing="false" text="Details">
            <HBox.margin>
                <Insets left="20.0" />
            </HBox.margin>
        </Button>
        <Button mnemonicParsing="false" onMouseClicked="#sayTest" text="Fire">
            <HBox.margin>
                <Insets left="20.0" />
            </HBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</HBox>
