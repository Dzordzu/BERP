   <menus>
      <Menu mnemonicParsing="false" text="Manage">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#gotoAdd" styleClass="menu-button" text="Add Employee" />
            <MenuItem mnemonicParsing="false" onAction="#gotoBillings" text="View Billings" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Help">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#gotoAbout" text="About" />
         </items>
      </Menu>
   </menus>
</MenuBar>
